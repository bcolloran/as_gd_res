[configuration]
entry_symbol = "gdext_rust_init"
compatibility_minimum = 4.1
reloadable = true

[libraries]
linux.debug.x86_64 =     "res://../target/debug/libresource_test_rust.so"
linux.release.x86_64 =   "res://../target/release/libresource_test_rust.so"
windows.debug.x86_64 =   "res://../target/debug/resource_test_rust.dll"
windows.release.x86_64 = "res://../target/release/resource_test_rust.dll"
macos.debug =            "res://../target/debug/libresource_test_rust.dylib"
macos.release =          "res://../target/release/libresource_test_rust.dylib"
macos.debug.arm64 =      "res://../target/debug/libresource_test_rust.dylib"
macos.release.arm64 =    "res://../target/release/libresource_test_rust.dylib"
